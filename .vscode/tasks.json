{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc Build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c99",
                "-Wall",
                "-Werror",
                "-Wextra",
                "-I.",
                "${file}",
                "ft_calloc.c",
                "ft_strlen.c",
                "ft_bzero.c",
                "ft_substr.c",
                "ft_strlcpy.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tâche générée par le débogueur."
        }
    ],
    "version": "2.0.0"
}